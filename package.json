{
    "name": "fastify-graphql",
    "version": "1.0.0",
    "description": "Fastify GraphQL",
    "main": "dist/index.js",
    "scripts": {
        "dev": "NODE_ENV=staging ts-node-dev --trace-warnings --respawn --no-notify --files --watch graphql --ignore-watch node_modules src/index.ts",
        "dev:prod": "ts-node-dev --trace-warnings --respawn --no-notify --files --watch graphql --ignore-watch node_modules src/index.ts",
        "start": "node dist/index.js",
        "build": "rm -rf dist && tsc -b",
        "debug": "node --inspect -r ts-node/register src/index.ts",
        "lint": "eslint . --ext .ts",
        "lint:fix": "yarn lint -- --fix",
        "test": "ava",
        "prisma:migrate": "prisma migrate dev --name"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/dannysofftie/fastify-graphql.git"
    },
    "keywords": [
        "fastify",
        "graphql",
        "prisma",
        "postgresql"
    ],
    "author": "Danny Sofftie",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dannysofftie/fastify-graphql/issues"
    },
    "homepage": "https://github.com/dannysofftie/fastify-graphql#readme",
    "dependencies": {
        "@prisma/client": "^2.19.0",
        "altair-fastify-plugin": "^3.2.2",
        "axios": "^0.21.1",
        "bcrypt": "^5.0.1",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.6",
        "fastify": "^3.14.0",
        "fastify-cors": "^5.2.0",
        "fastify-plugin": "^3.0.0",
        "fastify-static": "^4.0.1",
        "firebase-admin": "^9.5.0",
        "graphql-middleware": "^6.0.4",
        "graphql-shield": "^7.5.0",
        "graphql-tag": "^2.11.0",
        "html-minifier": "^4.0.0",
        "jsonwebtoken": "^8.5.1",
        "lru-cache": "^6.0.0",
        "mercurius": "^7.2.0",
        "mercurius-codegen": "^1.3.8",
        "module-alias": "^2.2.2",
        "node-xlsx": "^0.16.1",
        "puppeteer": "^8.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/ejs": "^3.0.6",
        "@types/html-minifier": "^4.0.0",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/lru-cache": "^5.1.0",
        "@types/module-alias": "^2.0.0",
        "@types/node": "^14.14.35",
        "@types/node-xlsx": "^0.15.1",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "ava": "^3.15.0",
        "eslint": "^7.22.0",
        "prisma": "^2.19.0",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.3"
    }
}
